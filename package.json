{
  "name": "build-function",
  "version": "0.1.0",
  "description": "The way to describe and build simple functions using JSON",
  "keywords": [
    "build",
    "function",
    "json"
  ],
  "main": "dist/build-function.js",
  "module": "dist/build-function.mjs",
  "browser": "dist/build-function.umd.js",
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "bundlib": {
    "name": "buildFunc",
    "min": "browser",
    "globals": {
      "object-hash": "objectHash"
    }
  },
  "scripts": {
    "dev": "bundlib -dw",
    "build": "bundlib -d",
    "test": "jest --color -w 3",
    "watch": "npm test -- --watch-all",
    "clean": "rimraf dist types coverage node_modules/.cache/bundlib node_modules/.cache/jest",
    "lint": "eslint src/**/*.ts __test__/**/*.ts",
    "prerelease": "npm run clean && npm run lint && npm test && npm run build",
    "release": "standard-version"
  },
  "dependencies": {
    "object-hash": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "@types/object-hash": "^1.3.1",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "bundlib": "^0.16.1",
    "chokidar": "^3.3.1",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "jest": "^25.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "standard-version": "^7.1.0",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "files": [
    "dist",
    "types"
  ],
  "author": {
    "name": "Manuel Fernandez",
    "email": "manferlo81@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manferlo81/build-function.git"
  },
  "bugs": {
    "url": "https://github.com/manferlo81/build-function/issues"
  },
  "homepage": "https://github.com/manferlo81/build-function#readme",
  "license": "MIT"
}
